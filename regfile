v0x1c5f410_0 .var "dutpassed", 0 0;
v0x1c5f510_0 .var "endtest", 0 0;
E_0x1c22ee0 .event posedge, v0x1c5f370_0;
    .scope S_0x1c229e0;
T_0 ;
    %set/v v0x1c5f1e0_0, 0, 32;
    %set/v v0x1c5efb0_0, 0, 5;
    %set/v v0x1c5f060_0, 0, 5;
    %set/v v0x1c5f280_0, 0, 5;
    %set/v v0x1c5f100_0, 0, 1;
    %set/v v0x1c36630_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x1c229e0;
T_1 ;
    %wait E_0x1c22ee0;
    %set/v v0x1c5f510_0, 0, 1;
    %set/v v0x1c5f410_0, 1, 1;
    %delay 10, 0;
    %movi 8, 2, 5;
    %set/v v0x1c5f280_0, 8, 5;
    %movi 8, 42, 32;
    %set/v v0x1c5f1e0_0, 8, 32;
    %set/v v0x1c5f100_0, 1, 1;
    %movi 8, 2, 5;
    %set/v v0x1c5efb0_0, 8, 5;
    %movi 8, 2, 5;
    %set/v v0x1c5f060_0, 8, 5;
    %delay 5, 0;
    %set/v v0x1c36630_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1c36630_0, 0, 1;
    %load/v 8, v0x1c5ee70_0, 32;
    %cmpi/u 8, 42, 32;
    %inv 4, 1;
    %mov 8, 4, 1;
    %load/v 9, v0x1c5ef10_0, 32;
    %cmpi/u 9, 42, 32;
    %inv 4, 1;
    %or 8, 4, 1;
    %jmp/0xz  T_1.0, 8;
    %set/v v0x1c5f410_0, 0, 1;
    %vpi_call 2 124 "$display", "Test Case 1 Failed";
T_1.0 ;
    %movi 8, 2, 5;
    %set/v v0x1c5f280_0, 8, 5;
    %movi 8, 15, 32;
    %set/v v0x1c5f1e0_0, 8, 32;
    %set/v v0x1c5f100_0, 1, 1;
    %movi 8, 2, 5;
    %set/v v0x1c5efb0_0, 8, 5;
    %movi 8, 2, 5;
    %set/v v0x1c5f060_0, 8, 5;
    %delay 5, 0;
    %set/v v0x1c36630_0, 1, 1;
    %delay 5, 0;
    %set/v v0x1c36630_0, 0, 1;
    %load/v 8, v0x1c5ee70_0, 32;
    %cmpi/u 8, 15, 32;
    %inv 4, 1;
    %mov 8, 4, 1;
    %load/v 9, v0x1c5ef10_0, 32;
    %cmpi/u 9, 15, 32;
    %inv 4, 1;
    %or 8, 4, 1;
    %jmp/0xz  T_1.2, 8;
    %set/v v0x1c5f410_0, 0, 1;
