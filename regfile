S_0x1c229e0 .scope module, "tester" "hw4testbench" 2 35, 2 77, S_0x1c22df0;
 .timescale 0 0;
v0x1c36630_0 .var "Clk", 0 0;
v0x1c5ee70_0 .alias "ReadData1", 31 0, v0x1c5fce0_0;
v0x1c5ef10_0 .alias "ReadData2", 31 0, v0x1c5fdb0_0;
v0x1c5efb0_0 .var "ReadRegister1", 4 0;
v0x1c5f060_0 .var "ReadRegister2", 4 0;
v0x1c5f100_0 .var "RegWrite", 0 0;
v0x1c5f1e0_0 .var "WriteData", 31 0;
v0x1c5f280_0 .var "WriteRegister", 4 0;
